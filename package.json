{
  "name": "setup-mint-action",
  "version": "1.0.7",
  "description": "This action sets up a Mint programming language.",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build -m src/index.js",
    "lint": "eslint src __tests__",
    "security:auth": "snyk auth",
    "security:test": "snyk test",
    "test": "nyc mocha __tests__",
    "snyk-protect": "snyk protect",
    "prepublish": "yarn run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabasoad/setup-mint-action.git"
  },
  "keywords": [
    "actions",
    "mint",
    "web",
    "frontend",
    "setup"
  ],
  "author": "Yevhen Fabizhevskyi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabasoad/setup-mint-action/issues"
  },
  "homepage": "https://github.com/fabasoad/setup-mint-action#readme",
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/tool-cache": "1.6.1",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@vercel/ncc": "0.26.1",
    "chai": "4.2.0",
    "eslint": "7.16.0",
    "eslint-config-google": "0.14.0",
    "git-branch-is": "4.0.0",
    "husky": "4.3.6",
    "mocha": "8.2.1",
    "mocha-param": "2.0.1",
    "nyc": "15.1.0",
    "sinon": "9.2.2",
    "snyk": "1.437.2"
  },
  "snyk": true,
  "husky": {
    "hooks": {
      "pre-commit": "git-branch-is -r \"^((?!main).)*$\" && yarn run build && git add dist/index.js",
      "pre-push": "yarn run lint && yarn test"
    }
  }
}
